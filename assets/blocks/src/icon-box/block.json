{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 3,
	"name": "sanibgb/san-icon-box",
	"version": "1.0.0",
	"title": "Icon Box",
	"description": "Displays visually appealing icon boxes with customizable icons, titles, and content.",
	"category": "common",
	"keywords": [
		"segal afzar nadin",
		"icon",
		"box"
	],
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"title": {
			"type": "string"
		},
		"content": {
			"type": "string"
		},
		"titleColor": {
			"type": "string",
			"default": "#000000"
		},
		"contentColor": {
			"type": "string",
			"default": "#666666"
		}
	},
	"textdomain": "san-icon-box-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"viewScript": "file:./view.js",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}